  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <alias alias="ITestUnit" type="TestProj.Interfaces.ITestUnit, TestProj"/>
    <alias alias="MainTest" type="TestProj.MainTest, TestProj"/>
    <alias alias="ScreenCapture" type="TestProj.Classes.ScreenCapture, TestProj"/>

    <sectionExtension
        type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension,
          Microsoft.Practices.Unity.Interception.Configuration"/>

    <container>
      <extension type="Interception"/>
      <register type="ITestUnit" mapTo="MainTest">
        <interceptor type="InterfaceInterceptor"/>
        <interceptionBehaviour type="ScreenCapture"/>
      </register>
    </container>
  </unity>
  
  <applicationSettings>
